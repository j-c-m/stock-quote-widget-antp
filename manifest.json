{
    "update_url": "http://clients2.google.com/service/update2/crx",
    "background": {
      "scripts": ["js/jquery.js", "js/background.js"],
      "persistent": false
    },
    "version": "2013.11.02.1",
    "name": "Real-Time Stock Quote Widget",
    "description": "Real-time stock quote widget for Awesome New Tab Page",
    "icons": {
      "128": "icon.png"
    },
    "manifest_version": 2,
    "web_accessible_resources": ["widget.html"],
    "content_security_policy": "script-src 'self' https://quote.cnbc.com; object-src 'self'",
    "permissions": [ "https://quote.cnbc.com/*", "storage" ],
    "options_page": "options.html"
}
